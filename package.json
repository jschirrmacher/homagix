{
  "name": "homagix",
  "version": "0.1.0",
  "description": "Makes food shopping easier",
  "repository": "https://github.com/jschirrmacher/homagix.git",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "build": "webpack --config config/webpack.conf.js",
    "lint": "eslint server/**/*.js src/**/*.js",
    "test-server": "nyc --reporter=lcov --reporter=text-summary mocha server/**/*.test.js",
    "test-app": "jest --env=jsdom src/**/*.test.js",
    "test": "CI=true npm run lint && npm run test-server && npm run test-app"
  },
  "author": "joachimschirrmacher@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "body-parser": "^1.19.0",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^3.0.1",
    "jest": "^25.5.4",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.2.0",
    "mock-fs": "^4.12.0",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "sass-loader": "^9.0.2",
    "should": "^13.2.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(js|jsx|ts)$": "babel-jest",
      "^.+\\.css|less$": "<rootDir>/config/fileTransformer.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!<MODULE_NAME>).+\\.js$"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:8080/"
}
