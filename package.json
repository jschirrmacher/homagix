{
  "name": "homagix",
  "version": "0.1.0",
  "description": "Makes food shopping easier",
  "repository": "https://github.com/jschirrmacher/homagix.git",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "build": "webpack --config config/webpack.conf.dev.js",
    "test-server": "eslint server/**/*.js && mocha server/**/*.test.js",
    "test-app": "eslint src/**/*.js && jest --env=jsdom src/**/*.test.js",
    "test": "CI=true npm run test-server && npm run test-app"
  },
  "author": "joachimschirrmacher@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "yaml": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "eslint": "^5.12.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "jest": "^24.0.0",
    "mocha": "^5.2.0",
    "mock-fs": "^4.7.0",
    "nyc": "^13.1.0",
    "react": "^16.6.3",
    "react-date-picker": "^7.1.1",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-tabs": "^2.3.0",
    "react-test-renderer": "^16.6.3",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "should": "^13.2.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.4.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(js|jsx|ts)$": "babel-jest",
      "^.+\\.css|less$": "<rootDir>/config/fileTransformer.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!<MODULE_NAME>).+\\.js$"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:8080/"
}
